  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250816.0",
    "@types/node": "^24.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.4.5",
    "wrangler": "^4.30.0"
  }
}